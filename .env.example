# Server port - The port on which the application will run (default: 3000)
PORT=3000

# App package name - Your application's unique identifier.  Make sure it exactly matches the package name in the MentraOS Developer Console
PACKAGE_NAME=org.yourname.appname

# API key - Your MentraOS developer API key from the MentraOS Developer Console (never commit the key to source control)
MENTRAOS_API_KEY=your_api_key_here

# Use Vite dev server (set to 'true' for local development, remove or set to 'false' for production)
USE_VITE_DEV=true

# Vite requires VITE_ prefix for frontend environment variables
VITE_AUDIO_URL=NGROK_LINK_HERE/assets/audio/one_more_time.mp3

# Master Agent Server Configuration
MASTER_AGENT_URL=http://localhost:3001
MASTER_AGENT_PORT=3001

# Anthropic API Key for Master Agent (required for AI features)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
# MongoDB Connection (required for persistence)
MONGODB_URI=mongodb+srv://your_connection_string

# Gemini API Key for LLM Observer (required for daemon)
GEMINI_API_KEY=your_gemini_api_key_here
