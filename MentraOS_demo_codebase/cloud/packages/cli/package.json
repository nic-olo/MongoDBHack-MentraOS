{
  "name": "@mentra/cli",
  "version": "1.0.0",
  "description": "Mentra CLI - Command-line tool for managing Mentra apps and organizations",
  "type": "module",
  "bin": {
    "mentra": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "scripts": {
    "build": "tsc && cp src/config/clouds.yaml dist/config/ && chmod +x dist/index.js",
    "dev": "bun run src/index.ts",
    "start": "bun run dist/index.js",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "mentra",
    "cli",
    "smart-glasses",
    "augmented-reality"
  ],
  "author": "Mentra",
  "license": "MIT",
  "dependencies": {
    "@mentra/types": "workspace:*",
    "commander": "^11.1.0",
    "axios": "^1.6.5",
    "jsonwebtoken": "^9.0.2",
    "cli-table3": "^0.6.3",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.11.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/inquirer": "^9.0.7",
    "typescript": "^5.3.3",
    "bun-types": "latest"
  },
  "engines": {
    "bun": ">=1.3.0"
  }
}
